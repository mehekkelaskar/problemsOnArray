class Main{
public static void main(String args[]){
int arr[]={2,3,5,45,26,78,99};
int max=arr[0],secmax=0;
for(int i=1;i<arr.length();i++){
if(arr[i]>max){
secmax=max;
max=arr[i];
}
else if(arr[i]>secmax &&arr[i]!=max){
secmax=arr[i];
}
}
System.out.println("Second largest element is  :"+secmax);
}
}
